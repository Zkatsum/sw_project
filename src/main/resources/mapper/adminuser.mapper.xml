<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--namespace命名空间，当前文档的一个标识符-->
<mapper namespace="com.qianfeng.sw.queen.dto.AdminUserMapper">
    <resultMap id="AdminUserResultMap" type="com.qianfeng.sw.queen.dto.AdminUserDTO">
        <id property="adminUserId" column="sw_admin_user_id" javaType="java.lang.Integer"/>
        <result property="adminUserUsername" column="sw_admin_user_username" javaType="java.lang.String"/>
        <result property="adminUserPassword" column="sw_admin_user_password" javaType="java.lang.String"/>
    </resultMap>

    <select id="checkUsername" resultMap="AdminUserResultMap">
        SELECT * FROM sw_admin_user WHERE sw_admin_user_username=#{adminUserUsername};
    </select>
</mapper>